{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": "off",
        "semi": ["error", "never"],
        "no-plusplus": "off",
        "no-console": "off"
    }
}
